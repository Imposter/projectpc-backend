{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "sourceMap": true,
        "noImplicitAny": true,
        "removeComments": true,
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules",
        "obj",
        "bin",
        ".vs"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
}